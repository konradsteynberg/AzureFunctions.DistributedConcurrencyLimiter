{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Trace",
      "Microsoft": "Error",
      "Azure": "Error",
      "System": "Error"
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:02",
      "visibilityTimeout": "00:00:30",
      "batchSize": 32,
      "maxDequeueCount": 5,
      "newBatchThreshold": 16,
      "messageEncoding": "base64"
    }
  }
}